{
  "name": "squeryll",
  "version": "1.0.0",
  "description": "A Cross-Platform SQL Client for Desktop",
  "main": "./dist/main.js",
  "scripts": {
    "check-types": "tsc --project tsconfig.json",
    "dev:electron": "wait-on tcp:3000 && cross-env NODE_ENV=development webpack --config webpack.electron.config.js && electron .",
    "dev:react": "cross-env NODE_ENV=development webpack-dev-server --config webpack.react.config.js",
    "dev": "concurrently -k \"npm run dev:react\" \"npm run dev:electron\"",
    "build:electron": "cross-env NODE_ENV=production webpack --config webpack.electron.config.js && electron .",
    "build:react": "cross-env NODE_ENV=production webpack --config webpack.react.config.js",
    "build": "npm run check-types && npm run build:react && npm run build:electron"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@types/estree": "^1.0.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "babel-loader": "^9.1.2",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "electron": "^22.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "typescript": "^4.9.5",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0"
  }
}
